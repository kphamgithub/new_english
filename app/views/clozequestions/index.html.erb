    <h2> For Admin only</h2>
	<% @clozequestions.each do |q| %>
	<p> Question id:
	<%= q.id %>
	</p>
    <p> Name: 
	<%= q.name %>
	</p>
	
	<p><%= link_to "Edit", edit_clozequestion_path(q.id) %></p>

	<%= button_to "Delete ", { controller: "clozequestions", action: "destroy", id: q.id }, method: :delete, data: { confirm: "Are you sure?" } %>
	<% end %>



