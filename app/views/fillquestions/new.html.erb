<%= form_for :fillquestion, url: fillquestions_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_area :name %>
  </p>
  <p>
    <%= f.label :question %><br>
    <%= f.text_area :question %>
  </p>
  
    <% if @vocabulary != nil %>
    <p> Voca name = <%= @vocabulary.name %></p>
    <p>
    <%= f.label :vocabulary%><br>
    <%= f.text_field :vocabulary, :value => @vocabulary.name %>
    </p>
   <% else %>
    <p>
    <%= f.label :vocabulary%><br>
    <%= f.text_field :vocabulary %>
    </p>    
   <% end %>
  <p>

  <p><%= text_field_tag :test %></p>
  <p>
  <%= check_box_tag "create_quizquestion" %>Add this question to this quiz:
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back to Lessons List', multichoicequestions_path %>