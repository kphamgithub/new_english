<%= form_for :vocabulary, url: vocabulary_path(@vocabulary), method: :patch do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :definition %><br>
    <%= f.text_field :definition %>
  </p>
  <p>
    <%= f.label :group %><br>
    <%= f.text_field :group %>
  </p>
    <p>
    <%= f.label :supergroup %><br>
    <%= f.text_field :supergroup %>
  </p>

  <p>
    <%= f.label :audio%><br>
    <%= f.text_field :audio %>
  </p>
      <p>
    <%= f.label :image%><br>
    <%= f.text_field :image %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back to Vocabulary List', vocabularies_path %>