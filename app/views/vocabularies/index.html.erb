<h1>Vocabularies Listing (for Admin only) </h1>
 
<table>
  <tr>
    <th> Name</th> 
    <th> Definition</th>
	<th> </th>  
  </tr>
 
  <% @vocabularies.each do |voca| %>
    <tr>
	  <td><%= voca.name %></td>
	  <td><%= voca.definition %></td>
	  <td><%= link_to "Edit", edit_vocabulary_path(voca.id) %></td>
	  <td><%= link_to "create questions", new_vocabulary_multichoicequestion_path(voca.id) %></td>
    </tr>
  <% end %>
</table>
<p><%= link_to "New Vocabulary", new_vocabulary_path %></p>
