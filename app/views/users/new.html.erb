<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :name %>
  </p>
 
	<p>
    <%= f.label :level%><br>
    <%= f.text_field :level %>
  </p> 

	<p>
    <%= f.label :lesson%><br>
    <%= f.text_field :lesson_id %>
  </p>  
  
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back to Users List', users_path %>