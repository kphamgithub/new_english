<%= form_for :user, url: user_path(@user), method: :patch do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :real_name %><br>
    <%= f.text_field :real_name %>
  </p>
 
  <p>
    <%= f.label :password%><br>
    <%= f.text_field :password %>
  </p>
     
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back to Users List', users_path %>